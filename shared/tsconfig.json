{
    "compilerOptions": {
        "lib": [
            "ESNext"
        ],
        "target": "ESNext",
        "module": "Node16",
        "moduleResolution": "Node16",
        "strict": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "exactOptionalPropertyTypes": true,
        "forceConsistentCasingInFileNames": true
    }
}
